          simulate() {
            axios
              .post("/simulate", { simulationTime: this.simulationTime })
              .then((res) => {
                debugger;
                this.logMain = res.data.logs.main;
                this.logResult = res.data.logs.result;
                this.stateLogs = res.data.stateLogs;
              });
          },
