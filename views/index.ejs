                  line_1: {
                    isActive: false,
                    queue: this.stateConfig.line_1.queue,
                  },
                  line_2: {
                    isActive: false,
                    queue: this.stateConfig.line_2.queue,
                  },
                };
                i++;
                if (i < this.stateLogs.length) iteration(i);
              }, 50);
            };
            this.$nextTick(() => {
              iteration(0);
            });
